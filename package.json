    "contentful": "^7.14.0",
    "cookieconsent": "^3.1.1",
    "express": "^4.17.1",
    "ngx-markdown": "^9.0.0",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "pg-native": "^3.0.0",
    "prismjs": "^1.19.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "tslib": "^2.0.0",
    "universal-analytics": "^0.4.20",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.2",
