{
  "name": "@deepkit/website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "watch": "ng build --watch",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "serve:ssr": "nodemon dist/server/main.js",
    "build:ssr": "ng build --progress=false --prod && ng run website:server:production",
    "server-module:watch": "ng run website:server --watch",
    "watch:ssr": "npm-run-all --parallel watch server-module:watch",
    "server": "ts-node --project ./src/server/tsconfig.json ./src/server/app.ts server:listen",
    "server:watch": "ts-node-dev --project ./src/server/tsconfig.json ./src/server/app.ts server:listen"
  },
  "type": "module",
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.2",
    "@angular/common": "^11.0.2",
    "@angular/compiler": "^11.0.2",
    "@angular/core": "^11.0.2",
    "@angular/elements": "^11.0.2",
    "@angular/forms": "^11.0.2",
    "@angular/platform-browser": "^11.0.2",
    "@angular/platform-browser-dynamic": "^11.0.2",
    "@angular/platform-server": "^11.0.2",
    "@angular/router": "^11.0.2",
    "@deepkit/core": "^1.0.1-alpha.13",
    "@deepkit/core-rxjs": "^1.0.1-alpha.13",
    "@deepkit/framework": "^1.0.1-alpha.13",
    "@deepkit/angular-universal": "^1.0.1-alpha.13",
